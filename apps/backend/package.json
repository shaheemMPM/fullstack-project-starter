{
	"name": "backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"dev": "dotenv -e ../../.env -- nest start --watch",
		"start": "node dist/main",
		"lint": "biome lint .",
		"test": "jest",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "dotenv -e ../../.env -- drizzle-kit migrate",
		"db:push": "dotenv -e ../../.env -- drizzle-kit push",
		"db:studio": "dotenv -e ../../.env -- drizzle-kit studio"
	},
	"dependencies": {
		"@nestjs/common": "^11.0.1",
		"@nestjs/core": "^11.0.1",
		"@nestjs/platform-express": "^11.0.1",
		"drizzle-orm": "^0.44.7",
		"postgres": "^3.4.7",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@types/express": "^5.0.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^22.10.7",
		"@types/supertest": "^6.0.2",
		"drizzle-kit": "^0.31.6",
		"jest": "^30.0.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
